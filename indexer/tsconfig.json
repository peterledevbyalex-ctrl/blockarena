{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["ES2022"],
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "generated": ["./generated"],
      "generated/*": ["./generated/*"]
    }
  },
  "include": ["src/**/*", "generated/**/*"],
  "exclude": ["node_modules"]
}
